{
  "name": "firma",
  "description": "clean",
  "version": "2.0.0",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp --dev",
    "build": "gulp --prod",
    "clone": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./run/clone.ps1",
    "deploy": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./run/deploy.ps1",
    "deploy.live": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./run/deploy.ps1 -live",
    "start.debug": "gulp --dev --debug",
    "build.debug": "gulp --prod --debug"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "3.1.1",
    "browser-sync": "2.26.7",
    "del": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "esm": "3.2.25",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-cache": "1.1.3",
    "gulp-concat": "2.6.1",
    "gulp-debug": "4.0.0",
    "gulp-eslint": "6.0.0",
    "gulp-favicons": "2.4.0",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "7.1.0",
    "gulp-phpcs": "3.1.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.0.2",
    "gulp-stylelint": "11.0.0",
    "gulp-uglify-es": "2.0.0",
    "minimist": "1.2.0",
    "node-sass": "4.13.1",
    "parcel-bundler": "1.12.4",
    "stylelint": "13.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.13.0",
    "vue-template-compiler": "2.6.11"
  },
  "dependencies": {
    "body-scroll-lock": "2.6.4",
    "headroom.js": "0.11.0",
    "document-register-element": "1.14.3",
    "jquery": "3.4.1"
  },
  "stylelint": {
    "extends": "./.stylelintrc",
    "ignoreFiles": [
      "dist/**",
      "backup/**"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "eslint-config-standard",
    "rules": {
      "camelcase": "off",
      "no-unneeded-ternary": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "spaced-comment": "off"
    }
  },
  "eslintIgnore": [
    "dist/**",
    "backup/**"
  ],
  "author": "Marian Schramm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrnschrmm/firma/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrnschrmm/firma.git"
  }
}
