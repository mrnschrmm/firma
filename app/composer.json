{
    "name": "mrnmrhcs/firma",
    "description": "Company Website",
    "require": {
        "getkirby/cms": "3.3.6",
        "sylvainjule/locator": "1.0.6",
        "sylvainjule/matomo": "1.0.5",
        "vlucas/phpdotenv": "4.1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.5.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "kirby-cms-path": "kirby",
        "kirby-plugin-path": "site/plugins"
    },
    "autoload": {
        "classmap": ["kirby/dependencies/"],
        "psr-4": {
            "Kirby\\": "kirby/src/"
        }
    }
}
